<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xir1n23.remidiuts.telkomschid.combinenavigation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SwipeViewActivity"
            android:label="@string/title_activity_swipe_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TabViewActivity"
            android:label="@string/title_activity_tab_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SpinnerViewActivity"
            android:label="@string/title_activity_spinner_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NavDrawerActivity"
            android:label="@string/title_activity_nav_drawer"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>